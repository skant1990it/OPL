 </script>
 
<body class="container" >
<header>
        <% include ../partials/dashheader %>
    </header>
    <main>
        <div class="row" id="backgrnd">
            <div class="col-sm-12">
            <div id="graph_div">
            <div id="container1"  class="graphcontainer" ></div>
              <div id="container2"  class="graphcontainer"  ></div>
                <div id="container3"  class="graphcontainer"  ></div>
                  <div id="container4"   class="graphcontainer"  ></div>
            </div>
                <div class="scoreboard" id="jumbotron">

                </div>
                
            </div>
            
        </div>
    </main>
    
    <div id="myModal" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog ">
        <div class="modal-content">
            <div class="modal-header" id="heading">
                <h4 class="start_match_title">Start Match Setting</h4>
            </div>
             <div class="modal-body toss-Match" style="display: none;">
            </div>
            <div class="modal-body start-Match">
            </div>
            <div class="modal-body over-details" id="overdetails">
            </div>
            <div class="modal-footer">
            	<button type="submit" class="btn btn-primary" id="start_match_btn">Start Match</button>
            	<button type="button" class="btn btn-primary" id="cancelbtn" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary back_to_home" id="back_to_home">Back to Home</button>
            </div>
        </div>
    </div>
</div>


<div id="resultModal" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog ">
        <div class="modal-content">
            <div class="modal-header" id="heading">
                <h4>Match status</h4>
            </div>
             <div class="modal-body result-Match">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary match_done" id="match_done">Back to Home</button>
            </div>
        </div>
    </div>
</div>